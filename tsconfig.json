{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "strictPropertyInitialization": false,
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "@modules/*":["modules/*"],
      "@config/*":["config/*"],
      "@shared/*":["shared/*"],
      "@database/*":["database/*"],
      "@routes/*":["routes/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
